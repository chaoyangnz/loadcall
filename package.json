{
  "name": "loadrush",
  "version": "0.2.0",
  "description": "Simple and flexible load testing tool by developers and for developers",
  "author": "Chao Yang <chaoyang.nz@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/loadflux/loadrush",
  "keywords": [
    "typescript",
    "nodejs",
    "load-testing",
    "stress-testing"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "node lib/index.js",
    "clean": "rimraf coverage build lib tmp",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prerelease": "npm run build",
    "release": "npm login && bump --tag && npm publish",
    "postrelease": "git push && git push --tags",
    "typecheck": "tsc --noEmit",
    "format": "prettier '**/*.{md,js,jsx,json,ts,tsx}' --write"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@influxdata/influx": "^0.5.5",
    "agentkeepalive": "^4.1.0",
    "async": "^3.1.0",
    "chalk": "^3.0.0",
    "content-type": "^1.0.4",
    "core-js": "^3.4.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eventemitter3": "^4.0.0",
    "expect": "^24.9.0",
    "form-data": "^3.0.0",
    "got": "^10.2.2",
    "jmespath": "^0.15.0",
    "jsonpath": "^1.0.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.25",
    "mustache": "^3.1.0",
    "ora": "^4.0.3",
    "tslib": "^1.9.3",
    "uuid": "^8.3.2",
    "xmlhttprequest": "^1.8.0",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/content-type": "^1.1.3",
    "@types/debug": "^4.1.5",
    "@types/follow-redirects": "^1.8.0",
    "@types/jest": "^24.0.23",
    "@types/jmespath": "^0.15.0",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.149",
    "@types/mime-types": "^2.1.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.12.12",
    "@types/pg": "^7.14.7",
    "@types/uuid": "^8.3.0",
    "docsify-cli": "^4.4.2",
    "jest": "^24.9.0",
    "pascal-case": "^2.0.1",
    "pg": "^8.5.1",
    "prettier": "^1.13.7",
    "prompts": "^2.4.1",
    "rimraf": "^3.0.0",
    "semver": "^6.3.0",
    "ts-jest": "~24.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.4",
    "version-bump-prompt": "^6.1.0"
  },
  "optionalDependencies": {
    "@types/faker": "^4.1.7",
    "aes256": "^1.0.4",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1"
  },
  "engines": {
    "node": ">= 12.20 <15"
  },
  "volta": {
    "node": "12.20.0"
  }
}
