{
  "name": "loadflux",
  "version": "0.1.0",
  "description": "Simple and flexible load testing tool by developers and for developers",
  "author": "Chao Yang <chaoyang.nz@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/loadflux/loadflux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist --module commonjs",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "lint": "tslint --project tsconfig.json --format stylish",
    "format": "prettier '**/*.{md,js,jsx,json,ts,tsx}' --write",
    "test": "jest --env=node --coverage",
    "test:watch": "jest --env=node --watch --updateSnapshot",
    "prepublish": "npm run build",
    "prebuild": "npm run clean",
    "postbuild": "rimraf {lib,es,dist}/**/__tests__ {lib,es,dist}/**/*.{spec,test,dist}.{js,d.ts,js.map}",
    "posttest": "npm run typecheck && npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "load-testing"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@influxdata/influx": "^0.5.5",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "chalk": "^3.0.0",
    "core-js": "^3.4.2",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.15",
    "ora": "^4.0.3",
    "tslib": "^1.9.3",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/debug": "^4.1.5",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.23",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.12",
    "@types/request": "^2.48.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "pascal-case": "^2.0.1",
    "prettier": "^1.13.7",
    "rimraf": "^3.0.0",
    "semver": "^6.3.0",
    "ts-jest": "~24.2.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.4"
  }
}
